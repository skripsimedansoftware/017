{
  "name": "exzly",
  "version": "0.0.1",
  "description": "Express.js Boilerplate",
  "main": "main.js",
  "type": "commonjs",
  "keywords": [
    "express.js",
    "boilerplate"
  ],
  "author": "Agung Dirgantara <agungmasda29@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node main.js",
    "start:dev": "nodemon",
    "db:demo": "sequelize-cli db:migrate --name base-1.0.0.js && sequelize-cli db:seed --seed demo",
    "db:start": "sequelize-cli db:migrate --name base-1.0.0.js && sequelize-cli db:seed --seed start",
    "lint": "eslint .",
    "format": "prettier --write --ignore-unknown .",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,json}": [
      "prettier --write --ignore-unknown",
      "eslint --fix"
    ]
  },
  "_moduleAliases": {
    "@exzly-config": "./src/config",
    "@exzly-helpers": "./src/helpers",
    "@exzly-middlewares": "./src/middlewares",
    "@exzly-models": "./src/models",
    "@exzly-routes": "./src/routes",
    "@exzly-utils": "./src/utils",
    "@exzly-validators": "./src/validators"
  },
  "dependencies": {
    "chokidar": "^3.6.0",
    "cli-table3": "^0.6.5",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "debug": "^4.4.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "express-list-endpoints": "^7.1.1",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.0",
    "file-type": "^19.6.0",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mime": "^3.0.0",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.46",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.11.5",
    "nodemailer": "^6.9.16",
    "nunjucks": "^3.2.4",
    "sequelize": "^6.37.5",
    "session-file-store": "^1.5.0",
    "sharp": "^0.33.5",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@faker-js/faker": "^9.3.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.1",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/mime": "^2.0.3",
    "@types/module-alias": "^2.0.4",
    "@types/multer": "^1.4.12",
    "@types/uuid": "^10.0.0",
    "concurrently": "^9.1.0",
    "eslint": "^9.16.0",
    "globals": "^15.13.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.9",
    "prettier": "3.4.1",
    "sequelize-cli": "^6.6.2"
  }
}
